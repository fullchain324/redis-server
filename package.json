{
  "name": "redis-server",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "index.js",
  "license": "MIT",
  "author": {
    "name": "Yurii"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fullchain324/redis-server.git"
  },
  "engines": {
    "node": ">=20.14.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "start": "node build/system/server.js",
    "clean": "rm -rf build",
    "build": "yarn clean && concurrently yarn:build:*",
    "build:compile": "npx babel src --extensions .ts --out-dir build --source-maps",
    "build:types": "tsc",
    "dev": "concurrently yarn:dev:* --kill-others \"nodemon -e node build/system/server.js\"",
    "dev:compile": "npx babel src --extensions .ts --out-dir build --source-maps --watch",
    "dev:types": "tsc --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "8.3.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "core-js": "3.37.1",
    "cors": "2.8.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "husky": "9.0.11",
    "redis": "4.6.14",
    "regenerator-runtime": "0.14.1",
    "socket.io": "4.7.5"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/node": "7.24.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-runtime": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@babel/runtime": "7.24.7",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@types/bluebird": "3.5.42",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/core-js": "2.5.8",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.8",
    "@types/redis": "4.0.11",
    "@types/regenerator-runtime": "0.13.6",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "babel-core": "6.26.3",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-source-map-support": "2.2.0",
    "concurrently": "8.2.2",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.2.0",
    "nodemon": "3.1.4",
    "prettier": "3.3.2",
    "ts-essentials": "10.0.1",
    "typescript": "5.5.2"
  }
}
